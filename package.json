{
  "name": "pixoprint",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "eleventy --serve",
    "build": "npm run clean && cross-env ELEVENTY_ENV=prod eleventy && cp assets/js/main.js docs/assets/js/main.js",
    "clean": "rimraf docs",
    "build:images": "gulp --gulpfile gulpfile.cjs",
    "clean:images": "gulp clean --gulpfile gulpfile.cjs",
    "deploy": "npm run build && git add docs && git commit -m \"Deploy\" && git push",
    "format": "prettier --write ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "cross-env": "^7.0.3",
    "del": "^6.1.1",
    "eleventy-plugin-i18n": "^0.1.3",
    "gh-pages": "^6.3.0",
    "gulp": "^4.0.2",
    "gulp-image-resize": "^0.13.1",
    "gulp-plumber": "^1.2.1",
    "gulp-webp": "^5.0.0",
    "prettier": "3.5.3",
    "rimraf": "^6.0.1",
    "serve": "^14.2.4",
    "sharp": "^0.31.3"
  }
